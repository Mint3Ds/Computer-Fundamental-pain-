// File name: tools/builtInChips/Or8Way.hdl
/**
 * 8-way Or gate: 
 * out = in[0] Or in[1] Or ... Or in[7]
 */
 CHIP Or16Way {

    IN  in[16];
    OUT out;

    PARTS:

    Or8Way(in=in[0..7], out=Or1);
    Or8Way(in=in[8..15], out=Or2);
    Or(a=Or1,b=Or2,out=out);
}
